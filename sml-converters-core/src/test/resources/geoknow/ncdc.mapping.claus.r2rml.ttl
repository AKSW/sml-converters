gkw:ncdc_ghcn_daily_snwd
    rr:logicalTable [ rr:sqlQuery "SELECT id, date, value AS snwd FROM ncdc_ghcn_daily WHERE element = 'SNWD' ;" ] ;
    rr:subjectMap [
        rr:class gkwo:WeatherObservation ;
        rr:template '{"id"}-{"date"}' ;
    ] ;
    rr:predicateObjectMap [
